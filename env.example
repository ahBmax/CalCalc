# Lead Capture Integration
LEAD_WEBHOOK_URL=https://your-webhook-url.com/leads
SUCCESS_REDIRECT_URL=/thank-you.html

# Email Service Integration (ConvertKit example)
CONVERTKIT_API_KEY=your_convertkit_api_key
